{
  "name": "redis-weighted-pool",
  "version": "0.1.0",
  "description": "Simple weighted round robin implementation using Redis list and sorted set",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "dependencies": {
    "@types/redis": "^2.8.6",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikesparr/redis-weighted-pool.git"
  },
  "keywords": [
    "redis",
    "load",
    "balancing",
    "weighted",
    "round",
    "robin",
    "routing",
    "object pool"
  ],
  "author": "Mike Sparr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesparr/redis-weighted-pool/issues"
  },
  "homepage": "https://github.com/mikesparr/redis-weighted-pool#readme"
}
